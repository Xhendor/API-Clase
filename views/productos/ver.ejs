<%- include("../header");%>
<div class="col-12">
    <h1>Productos</h1>
    <table class="table table-bordered">
        <thead>
            <th>Nombre</th>
            <th>Precio</th>
        </thead>

        <tbody>
            <% productos.forEach(producto =>{ %>

                <tr>
                    <td><%= producto.nombre %></td>
                    <td><%= producto.precio %></td>
                </tr>

            <% }) %>    
        </tbody>
    </table>
</div>
<%- include("../footer");%>